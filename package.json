{
  "name": "stream-automate",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "start": "nodemon index.ts",
    "clean": "rimraf ./build && rimraf ./dist",
    "copy-static": "copyfiles -u 1 ./static/**/*.* ./dist",
    "build": "tsc --project ./",
    "dist": "npm run clean && npm run build && pkg ./package.json && npm run copy-static"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.17.1",
    "obs-websocket-js": "^4.0.2",
    "robotjs": "^0.6.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.12",
    "@types/node": "^15.12.2",
    "copyfiles": "^2.4.1",
    "nodemon": "^2.0.7",
    "pkg": "^5.2.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2"
  },
  "bin": "build/index.js",
  "pkg": {
    "scripts": "build/**/*.js",
    "targets": [
      "node14-win-x64"
    ],
    "outputPath": "dist"
  }
}
