{
	"name": "stream-automate",
	"version": "1.1.0",
	"description": "",
	"main": "index.ts",
	"repository": {
		"type": "git",
		"url": "https://github.com/hlemont/stream-automate.git"
	},
	"scripts": {
		"start": "nodemon --exec npx ts-node index.ts",
		"clean": "rimraf ./build && rimraf ./dist",
		"copy-static": "copyfiles -u 1 ./static/**/*.* ./dist",
		"build": "tsc --project ./",
		"dist": "npm run clean && npm run build && pkg ./package.json && npm run copy-static"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@nut-tree/nut-js": "^2.0.0",
		"express": "^4.17.1",
		"express-async-handler": "^1.1.4",
		"obs-websocket-js": "^4.0.3"
	},
	"devDependencies": {
		"@types/express": "^4.17.12",
		"@types/node": "^15.12.2",
		"copyfiles": "^2.4.1",
		"joi": "^17.4.0",
		"nodemon": "^2.0.7",
		"pkg": "^5.2.1",
		"rimraf": "^3.0.2",
		"ts-node": "^10.4.0",
		"typescript": "^4.5.4"
	},
	"bin": "build/index.js",
	"pkg": {
		"scripts": "build/**/*.js",
		"targets": [
			"node14-win-x64"
		],
		"outputPath": "dist"
	}
}
